{
  "name": "pose-capture",
  "version": "1.0.0",
  "type": "module",
  "dependencies": {
    "@mediapipe/drawing_utils": "^0.3.1620248257",
    "lit": "^2.2.4"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@mediapipe/hands": "^0.4.1635986972",
    "@mediapipe/pose": "0.4.1633558788",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@tensorflow-models/body-pix": "^2.2.0",
    "@tensorflow-models/face-landmarks-detection": "0.0.3",
    "@tensorflow-models/hand-pose-detection": "^0.0.1",
    "@tensorflow-models/pose-detection": "0.0.6",
    "@tensorflow/tfjs-backend-webgl": "^3.13.0",
    "@tensorflow/tfjs-converter": "^3.13.0",
    "@tensorflow/tfjs-core": "^3.13.0",
    "@types/dom-mediacapture-record": "^1.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@web/dev-server": "^0.1.31",
    "eslint": "^8.8.0",
    "koa-range": "^0.3.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.0",
    "rollup-plugin-cpy": "^1.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-summary": "^1.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.5.5",
    "web-component-analyzer": "^1.1.6"
  },
  "keywords": [
    "web-components",
    "typescript",
    "tensorflowjs"
  ],
  "author": "Ben Farrell",
  "license": "MIT",
  "scripts": {
    "bundle-tensorflow": "./node_modules/.bin/rollup -c rollup.configs/tensorflow-cfg.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf video-element.{d.ts,d.ts.map,js,js.map}",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "docs": "npm run docs:clean && npm run build && npm run analyze && npm run docs:build && npm run docs:assets && npm run docs:gen",
    "docs:clean": "rimraf docs",
    "docs:gen": "eleventy --config=.eleventy.cjs",
    "docs:gen:watch": "eleventy --config=.eleventy.cjs --watch",
    "docs:build": "rollup -c --file docs/video-element.bundled.js",
    "docs:assets": "cp node_modules/prismjs/themes/prism-okaidia.css docs/",
    "docs:serve": "wds --root-dir=docs --node-resolve --watch",
    "analyze": "wca analyze \"src/**/*.ts\" --outFile custom-elements.json",
    "serve": "tsc --watch & wds --watch",
    "checksize": "rollup -c ; cat video-element.bundled.js | gzip -9 | wc -c ; rm video-element.bundled.js"
  }
}
